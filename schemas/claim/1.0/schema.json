{
  "$id": "claim-schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Claim",
  "type": "object",
  "required": [
    "name",
    "code"
  ],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 5
    },
    "code": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "Live",
        "Retired"
      ],
      "default": "Live"
    }
  },
  "additionalProperties": true
}